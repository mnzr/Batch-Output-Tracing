#include <stdio.h>
void change(int *a, int sz, int inc) {
    int i;
    for(i=0; i<sz; i++)
    *(a+i) = *(a+i) + inc;
}
int main() {
    int a[]={1,3,5,7,9,11,-4,8,-9,0};
    int i;
    change(a,10,3);
    for(i=0; i<3; i++)
    printf("%d\n", *(a+i));
    return 0;
}
