#include <stdio.h>
int main() {
    int i, j, n[3][3] = {2,4,3,6,8,5,3,5,1};
    for(i=0; i<=2; i++)
        for(j=0; j<=2; j++)
            printf("%d %d %d\n",n[i][i],n[j][j]+n[j][i],*(*(n+i)+j));
    return 0;
}
