#include <stdio.h>
struct life {
    int num;
    char msg1[50];
    char msg2[50];
} m1 = {
    4,
    "Life is a joyous journey for me",
    "Each search must end with a new quest"
};
int main() {
    struct life m2, m3;
    m2 = m1;
    m3 = m2;
    printf("%d %s %s\n", m1.num, m2.msg1, m3.msg2);
    return 0;
}
