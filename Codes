#include <stdio.h>
char* checkNumber(int num) {
    int i=0;
    for(i=2;i<num-1;i++) {
        if(num%i==0)
        { return "false"; }
    }
    return "true";
}adjsdf
int main () {
    int count;
    for(count=10;count<=20;count++) {
        puts(checkNumber(count));
    }
    return 0;
}
